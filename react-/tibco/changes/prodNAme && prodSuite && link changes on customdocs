componentWillReceiveProps(nextProps){
      // console.log("NXTPROPSSSSSS",nextProps);
      var pName = nextProps.product_data.result.product.name.replace(/[0-9]/g, '');
      pName = pName.replace(/\./g,"").trim();
      this.setState({prodNameFromNextProps:pName});








var cloudProdNamesArr = [
        'TIBCO Cloud Bus™',
        "TIBCO Cloud Messaging Documentation",
        "TIBCO Cloud™ Mashery Documentation",
        "TIBCO Cloud™ Spotfire® Documentation", 
        "TIBCO Cloud™ Administration Documentation", 
      ];

      $.each(productList, function(key, value) {
        // console.log("KEY+++++VAL",key,"+++++", value) 
        var prodSuiteKey=key
        $.each(value, function(key, value) {
          if(value == productName){
            productSuiteKey=prodSuiteKey
            $.each(productSuiteList, function(key, value) {
              $.each(value, function(childKey,val){
                if(val == productSuiteKey){
                  productSuiteVal.push(key)
                }
              })
            });
          }
        })
        
      });

      console.log("ppppppppppppppppppppppppppppppp",productSuiteVal);
      debugger;
      if(cloudProdNamesArr.indexOf(this.state.prodNameFromNextProps) != -1) {
        linkUrl = "/products/search#q="+this.state.searchQuery+"&prdName="+productName+"&t=Docs";
        localStorage.setItem("lastname", "/products/search#q="+this.state.searchQuery+"&prdName="+productName+"&t=Docs");
      }
      else { 
        // if(checkFlag == 0){
        // console.log("level 2")
        if(productSuiteVal && productSuiteVal.length>0){
          console.log("level 2.1 non cloud with suite")
          linkUrl = '/products/search#q='+this.state.userValue+'&t=Docs&f:@productsuite=['+
          productSuiteVal+']&f:@productname=['+productName+']&f:@prdversorted=['+normalizedVersion+']','_blank';
          }
        // }
        else {
          // if(checkFlag == 0){
            console.log("level 3 non cloud withOUT suite")
              linkUrl = '/products/search#q='+this.state.userValue+'&t=Docs&f:@productname=['+productName+']&f:@prdversorted=['+
              normalizedVersion+']','_blank';
          // }
        }    
      }
