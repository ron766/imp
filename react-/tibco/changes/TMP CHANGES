import React from 'react'
import Header from '../views/header';
import Banner from '../views/landing_page/banner';
import Footer from '../views/footer';
import ReactDOM from 'react-dom';

import {
    Accordion,
    AccordionItem,
    AccordionItemTitle,
    AccordionItemBody,
} from 'react-accessible-accordion';

class NewFaqPage extends React.Component { 
  render() {
    return ( 
      <div className = "faqq" >
        <Header />
        <Banner />
